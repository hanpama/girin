{
  "name": "girin",
  "version": "0.5.0-alpha.3",
  "description": "GraphQL bootstrapper up and running",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "../../node_modules/.bin/jest",
    "coverage": "../../node_modules/.bin/jest --coverage",
    "build": "tsc -p ./tsconfig.publish.json",
    "prepublish": "npm run build"
  },
  "keywords": [
    "graphql"
  ],
  "license": "MIT",
  "author": "Max Choi <hanpama@gmail.com>",
  "tags": [
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanpama/girin.git"
  },
  "bugs": {
    "url": "https://github.com/hanpama/girin/issues"
  },
  "homepage": "https://github.com/hanpama/girin",
  "dependencies": {
    "apollo-server": "^2.0.8",
    "girin-auth": "^0.5.0-alpha.3",
    "girin-model": "^0.5.0-alpha.3",
    "girin-typelinker": "^0.5.0-alpha.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.ts",
      "**/test/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/test/fixtures/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  },
  "gitHead": "7f87fc9641454bbe0e6367e5b1f15eea4a6fa956"
}
