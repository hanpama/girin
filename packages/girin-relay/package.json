{
  "name": "girin-relay",
  "version": "0.3.1",
  "description": "Girin relay integration",
  "author": "Max Choi <hanpama@gmail.com>",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "../../node_modules/.bin/tsc -p tsconfig.publish.json",
    "test": "../../node_modules/.bin/jest",
    "coverage": "../../node_modules/.bin/jest --coverage"
  },
  "tags": [
    "graphql"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanpama/girin.git"
  },
  "bugs": {
    "url": "https://github.com/hanpama/girin/issues"
  },
  "homepage": "https://github.com/hanpama/girin",
  "license": "MIT",
  "dependencies": {
    "girin": "^0.3.1",
    "graphql-relay": "^0.5.4"
  },
  "devDependencies": {
    "@types/graphql-relay": "^0.4.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.ts",
      "**/test/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/test/fixtures/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
