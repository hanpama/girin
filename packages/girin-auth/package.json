{
  "name": "girin-auth",
  "version": "0.5.0-alpha.1",
  "description": "girin auth package",
  "main": "index.js",
  "module": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "test": "../../node_modules/.bin/jest",
    "coverage": "../../node_modules/.bin/jest --coverage",
    "build": "../../node_modules/.bin/tsc -p tsconfig.publish.json",
    "prepublish": "npm run build"
  },
  "keywords": [
    "graphql",
    "mongodb",
    "dataloader"
  ],
  "license": "MIT",
  "author": "Max Choi <hanpama@gmail.com>",
  "tags": [
    "graphql",
    "mongodb",
    "dataloader"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hanpama/mongorelay.git"
  },
  "bugs": {
    "url": "https://github.com/hanpama/mongorelay/issues"
  },
  "homepage": "https://github.com/hanpama/mongorelay",
  "devDependencies": {
    "@types/jsonwebtoken": "^7.2.8"
  },
  "dependencies": {
    "argon2": "^0.19.3",
    "girin-model": "^0.5.0-alpha.1",
    "girin-typelinker": "^0.5.0-alpha.1",
    "jsonwebtoken": "^8.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/test/**/*.test.ts",
      "**/test/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/test/fixtures/"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
